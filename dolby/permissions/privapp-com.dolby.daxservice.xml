<?xml version="1.0" encoding="utf-8"?>
<permissions xmlns:android="http://schemas.android.com/apk/res/android">
    <privapp-permissions package="com.dolby.daxservice">
        <!-- Android Standard permissions -->
        <permission name="android.permission.READ_EXTERNAL_STORAGE" />
        <permission name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <permission name="android.permission.BLUETOOTH" />
        <permission name="android.permission.MODIFY_AUDIO_SETTINGS" />
        <permission name="android.permission.INTERACT_ACROSS_USERS" />
        <permission name="android.permission.INTERACT_ACROSS_USERS_FULL" />
        <permission name="android.permission.MANAGE_USERS" />
        <permission name="android.permission.SYSTEM_ALERT_WINDOW" />
        <permission name="android.permission.SUBSTITUTE_NOTIFICATION_APP_NAME" />
        <permission name="android.permission.WRITE_SECURE_SETTINGS" />
        <permission name="android.permission.QUERY_ALL_PACKAGES" />
        <!-- Twilight Manager -->
        <permission name="android.permission.ACCESS_COARSE_LOCATION" />
        <permission name="android.permission.ACCESS_FINE_LOCATION" />
        <!-- VQE QS Tile -->
        <permission name="android.permission.BIND_QUICK_SETTINGS_TILE" />
        <!-- Protected Broadcast -->
        <protected-broadcast android:name="com.dolby.intent.action.DAP_PARAMS_UPDATE"/>
        <protected-broadcast android:name="com.dolby.intent.ACTION_RELOAD_TUNING"/>
        <protected-broadcast android:name="audio_server_restarted"/>
    </privapp-permissions>
</permissions>
